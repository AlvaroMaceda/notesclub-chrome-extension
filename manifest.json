{
  "name": "Notes Club",
  "version": "0.1.5",
  "description": "https://notes.club is all about social notes.",
  "background": {
    "scripts": ["parameterize.js", "background.js"]
  },
  "permissions": [
    "https://api.notes.club/v1/topics/count",
    "tabs"
  ],
  "icons": {
      "16": "icon_16.png",
      "24": "icon_24.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_name": "Notes.Club occurrences"
  },
  "manifest_version": 2
}
